{
  "name": "@justaos/orm",
  "version": "7.1.1",
  "exports": "./mod.ts",
  "tasks": {
    "coverage": "deno task test --coverage=./dist/coverage && deno coverage --include=src --lcov ./dist/coverage > ./dist/coverage/lcov.info",
    "test": "deno test --allow-read --allow-env --allow-net --unstable-temporal",
    "bench": "deno test --allow-read --allow-env ./test/bench.ts"
  },
  "exclude": [
    "dist",
    ".run",
    ".github/",
    "version.json"
  ],
  "publish": {
    "exclude": [
      "test/"
    ]
  },
  "test": {
    "include": [
      "test/init.test.ts",
      "test/unit/",
      "test/end.test.ts"
    ]
  },
  "imports": {
    "@justaos/utils": "jsr:@justaos/utils@^2.0.0",
    "@std/assert": "jsr:@std/assert@^0.220.1",
    "@std/testing": "jsr:@std/testing@^0.220.1",
    "@utility/types": "jsr:@utility/types@^0.4.0",
    "pg": "npm:pg@^8.11.5",
    "pg-cursor": "npm:pg-cursor@^2.10.5",
    "pg-escape": "npm:pg-escape@^0.2.0"
  }
}
