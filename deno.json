{
  "name": "@justaos/orm",
  "version": "6.6.0",
  "exports": "./mod.ts",
  "tasks": {
    "test:debug": "deno test --inspect-brk --allow-all --unstable",
    "coverage": "deno task test --coverage=./dist/coverage && deno coverage --include=src --lcov ./dist/coverage > ./dist/coverage/lcov.info",
    "test": "deno test --allow-read --allow-env --allow-net --unstable-temporal"
  },
  "exclude": [
    ".run",
    "dist"
  ],
  "publish": {
    "exclude": [
      ".github/",
      "test/"
    ]
  },
  "test": {
    "include": [
      "test/init.test.ts",
      "test/unit/",
      "test/end.test.ts"
    ]
  },
  "imports": {
    "@justaos/utils": "jsr:@justaos/utils@^1.11.0",
    "@std/assert": "jsr:@std/assert@^0.220.1",
    "@std/testing": "jsr:@std/testing@^0.220.1"
  }
}
